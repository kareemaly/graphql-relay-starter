{
  "name": "graphql-relay-starter",
  "version": "0.0.1",
  "scripts": {
    "start:graphql": "concurrently \"npm run watcher:graphql\" \"npm run serve:graphql\"",
    "start:app": "concurrently \"npm run watcher:app\" \"npm run serve:app\"",
    "watcher:graphql": "nodemon --watch server/graphql/**/*.js --exec \"npm run update-graphql-schema\"",
    "watcher:app": "npm run update-relay",
    "serve:graphql": "nodemon --watch server --exec \"node server graphql\"",
    "serve:app": "node server app",
    "build:prod": "cross-env NODE_ENV=production npm run clean && npm run update-graphql-schema && npm run build:app ",
    "clean": "rimraf build",
    "update-graphql-schema": "node internals/updateSchema.js",
    "update-relay": "relay-compiler --watch --src ./app --schema server/graphql/__generated__/schema.json",
    "build:app": "webpack --config internals/webpack/webpack.prod.config.js --color -p --progress",
    "start:prod": "cross-env NODE_ENV=production node server",
    "heroku-postbuild": "npm run build:prod",
    "lint": "eslint app/**/*.js",
    "lint:fix": "eslint app/**/*.js --fix"
  },
  "_moduleAliases": {
    "AppIoC": "server/IoC",
    "@server": "server"
  },
  "engines": {
    "node": "~6.9.0",
    "npm": "3.10.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eventsource-polyfill": "^0.9.6",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.11.4",
    "nodemon": "^1.11.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.4.2",
    "relay-compiler": "1.4.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^2.0.1",
